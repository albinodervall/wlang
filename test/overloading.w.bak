#import "library/assert.w"

f :: (a: i32, b: i32): i32 {
    return a + b;
}

f :: (a: u32, b: u32): u32 {
    return a * b;
}

g :: (a: u32, b: u32): u32 {
    return a - b;
}

main :: () #entry {
    // assert(add(2, 3) == 5); // not ok, ambiguous!
    assert(f(2 as i32, 3) == 5);
    assert(f(2 as u32, 3) == 6);
    assert(g(6, 3) == 3);

    print("Ok\n");
}
